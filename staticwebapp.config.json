{
    "routes": [
      {
        "route": "/login",
        "rewrite": "/.auth/login/github"
      },
      {
        "route": "/logout",
        "rewrite": "/.auth/logout"
      },
      {
        "route": "/need/to/be/authenticated",
        "allowedRoles": ["authenticated"]
      }
    ],
    "responseOverrides": {
      "401": {
        "redirect": "/login",
        "statusCode": 302
      },
      "403": {
        "redirect": "/login",
        "statusCode": 302
      }
    }
  }